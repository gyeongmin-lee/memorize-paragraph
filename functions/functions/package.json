    "express": "^4.17.3",
    "firebase": "^6.2.4",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.3.0"
  },
  "depvDependencies": {
    "eslint": "^6.1.0",
    "typescript": "^3.2.2"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
